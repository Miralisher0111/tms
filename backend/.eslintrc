{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "unused-imports"
  ],
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "max-len": [
      "error",
      {
        "code": 150,
        "ignoreComments": true
      }
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "require-jsdoc": "off",
    "prefer-const": "warn",
    "arrow-parens": "off",
    "no-extra-semi": "off",
    "no-trailing-spaces": "error",
    "comma-dangle": "off",
    "new-cap": [
      "error",
      {
        "capIsNew": false
      }
    ],
    "linebreak-style": 0,
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ],
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
